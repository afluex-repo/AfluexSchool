@model AfluexSchool.Models.Reports

@{
    ViewBag.Title = "GetMultipleStudentByParent";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

 

@using (Html.BeginForm("GetMultipleStudentByParent", "AdminReports", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
{
    <div>
        <div class="card border-0 shadow-sm mb-4">
            <div class="card-header border-0 bg-none">
                <div class="row">
                    <div class="col-12 col-md">
                        <p class="fs15"> Students</p>
                    </div>
                </div>
            </div>
            <div class="card-body ">
                <div class="col-md-6">
                    <br />
                   <a class="btn btn-primary" href="/AdminReports/MultipleStudentReport">Back</a> 
                </div>
                <br />
                <div class="row">
                    <div class="table-responsive">
                        <table class="table table-bordered w-100 mb-0" id="example24">
                            <thead>
                                <tr>
                                    <th>S.no</th>
                                    <th>Login ID</th>
                                    <th>Name</th>
                                    <th>Class</th>
                                    <th>Section</th>
                                    <th>Registration No.</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model != null)
                                {
                                    if (Model.lstfeedata != null)
                                    {
                                        int i = 1;
                                        foreach (var item in Model.lstfeedata)
                                        {

                                            <tr>
                                                <td>
                                                    @i
                                                </td>
                                                <td>
                                                    @item.LoginId
                                                </td>
                                                <td>
                                                    @item.StudentName
                                                </td>
                                                <td>
                                                    @item.ClassName
                                                </td>
                                                <td>
                                                    @item.SectionName
                                                </td>
                                                <td>
                                                    @item.RegistrationAmt
                                                </td>
                                            </tr>
                                            i++;
                                        }
                                    }
                                }
                            </tbody>
                        </table>


                        <div class="j-response"></div>

                    </div>

                </div>

            </div>
        </div>
    </div>




}
