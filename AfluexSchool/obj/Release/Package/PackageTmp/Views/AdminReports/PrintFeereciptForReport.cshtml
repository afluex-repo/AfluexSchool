@model AfluexSchool.Models.Reports

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Print Student Fee</title>
    <style>
        table, th, td {
            font-size: medium;
            /*border: 1px solid black;*/
            border-collapse: collapse;
        }

        th, td {
            font-size: medium;
            padding: 5px;
        }
    </style>
    <link href="../../AdminPanelcss/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="../../AdminPanelcss/assets/vendor/bootstrap/css/style.css">
