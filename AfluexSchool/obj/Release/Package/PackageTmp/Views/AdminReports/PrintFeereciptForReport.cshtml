                                <td style="border: 1px solid black;" colspan="2"><b></b></td>
                                <td style="border: 1px solid black;text-align:right" colspan="1"><b></b></td>

                                <td style="border: 1px solid black;text-align:right" colspan="1"><b>Total</b></td>
                                <td style="border: 1px solid black;text-align:right" colspan="2"><b>@ViewBag.FinalAmtAfterCoon</b></td>


