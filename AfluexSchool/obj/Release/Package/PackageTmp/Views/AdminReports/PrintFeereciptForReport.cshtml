                                <td style="border: 1px solid black" colspan="2"><b>FeeType</b></td>
                                <td style="border: 1px solid black;text-align:right" colspan="1"><b>Inst Amt</b></td>
                                <td style="border: 1px solid black;text-align:right" colspan="1"><b>Concession</b></td>
                                <td style="border: 1px solid black;text-align:right" colspan="1"><b>Final Amt</b></td>



                            </tr>
                            @if (Model != null)
                            {
                                if (Model.lstfeedata != null)
                                {
                                    foreach (var item in Model.lstfeedata)
                                    {
                                        <tr>

                                            <td style="border: 1px solid black;" colspan="2">@item.FeeTypeName</td>

                                            <td style="border: 1px solid black;text-align:right" colspan="1">@item.InstallmentAmt</td>
                                            <td style="border: 1px solid black;text-align:right" colspan="1">@item.ConcessionFee</td>
                                            <td style="border: 1px solid black;text-align:right" colspan="1">@item.FinalAmount</td>

