    @* <div><a hre f="/Fee/Fees" class="btn btn-primary">BACK</a></div>*@
    <div id="printDiv">  &nbsp;&nbsp;<input type="button" onclick="printreport();" value="PRINT" /></div>

    <div class="main-recipt">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-6 col-lg-6">
                    <div class="main-recipt-logo">
                        <div class="recipt-logo1">
                            <div class="row">
                                <div class="col-md-2 col-lg-2">
                                    <div class="current-recipt-logo">

                                        <img src="../../Websitecss/images/favicon.png" class="img-responsive" />
                                    </div>
                                </div>
                                <div class="col-md-10 col-lg-10">
                                    <div class="current-recipt-logo">
                                        <h4>AFLUEX SCHOOL</h4>
                                        <h6>@ViewBag.CompanyAddress (@ViewBag.State)-@ViewBag.Pin1</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="recipt-logo-contact">
                            <div class="row">
                                <div class="col-md-12 col-lg-12">
                                    <div class="section-recipt1">
                                        <h6>Affiliation No. @ViewBag.AffliateNo <i class="fa fa-phone-square" aria-hidden="true"></i> @ViewBag.ContactNo <i class="fa fa-envelope-square" aria-hidden="true"></i> @ViewBag.EmailID</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="duplicate-fee-recipt">
                            <div class="row">
                                <div class="col-md-6 col-lg-6">
                                    <h6> Fee Receipt</h6>
                                </div>
                                <div class="col-md-6 col-lg-6">
                                    <h6>Session : @ViewBag.SessionName</h6>
                                </div>
                            </div>
                        </div>
                        <!-- duplicate-fee-recipt end -->
                        <div class="recipt-no">
                            <div class="row">
                                <div class="col-md-6 col-lg-6">
                                    <h6>Recipt No : @ViewBag.ReceiptNo</h6>
                                </div>

                                <div class="col-md-6 col-lg-6">
                                    <h6>Date : @ViewBag.AddedOn</h6>
                                </div>
                            </div>
                        </div>
                        <!-- recipt-no end -->
                        <div class="recipt-details">
                            <div class="row">
                                <div class="col-md-6 col-lg-6">
                                    <h6>REG No:@ViewBag.RegistrationNo</h6>
                                </div>

                                <div class="col-md-6 col-lg-6">
                                    <h6>Class:@ViewBag.ClassSection</h6>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 col-lg-6">
                                    <h6>Name: @ViewBag.StudentName</h6>
                                </div>
                                <div class="col-md-6 col-lg-6">
                                    <h6>DOB:@ViewBag.DateOfBirth</h6>
                                </div>
                                <div class="col-md-12 col-lg-12">
                                    <h6>Father Name : @ViewBag.FatherName</h6>
                                </div>
                                <div class="col-md-12 col-lg-12">
                                    <h6>Address : @ViewBag.Address</h6>
                                </div>
                            </div>
                        </div>
                        <!-- fee-month start -->
