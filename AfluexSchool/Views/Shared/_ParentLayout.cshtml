<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>@ViewBag.Title</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/AssetsAdmin/images/favicon.png">
    <link rel="stylesheet" href="~/AssetsAdmin/vendor/bootstrap-select/dist/css/bootstrap-select.min.css">
    <!-- Datatable -->
    <link href="~/AssetsAdmin/vendor/datatables/css/jquery.dataTables.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/AssetsAdmin/css/style.css">
    <link rel="stylesheet" href="~/AssetsAdmin/css/skin.css">
</head>

<body>

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->
    <!--******************Main wrapper start*************-->
    <div id="main-wrapper">
        <div class="nav-header">
            <a href="/ParentPanel/DashBoard" class="brand-logo">
                <img class="logo-abbr" src="~/AssetsAdmin/images/logo-white-2.png" alt="">
                <img class="logo-compact" src="~/AssetsAdmin/images/logo-text-white.png" alt="">
                <img class="brand-title" src="~/AssetsAdmin/images/logo-text-white.png" alt="">
            </a>

            <div class="nav-control">
                <div class="hamburger">
                    <span class="line"></span><span class="line"></span><span class="line"></span>
                </div>
            </div>
        </div>
        <div class="header">
            <div class="header-content">
                <nav class="navbar navbar-expand">
                    <div class="collapse navbar-collapse justify-content-between">
                        <div class="header-left">
                            @*<div class="search_bar dropdown">
                                <span class="search_icon p-3 c-pointer" data-toggle="dropdown">
                                    <i class="mdi mdi-magnify"></i>
                                </span>
                                <div class="dropdown-menu p-0 m-0">
                                    <form>
                                        <input class="form-control" type="search" placeholder="Search"
                                               aria-label="Search">
                                    </form>
                                </div>
                            </div>*@
                        </div>

                        <ul class="navbar-nav header-right">
                            <li class="nav-item dropdown header-profile">
                                <a class="nav-link" href="#" role="button" data-toggle="dropdown">
                                    <img src="@Session["UserImage"]" width="20" alt="">
                                </a>
                                <div class="dropdown-menu dropdown-menu-right">
                                    <a href="#!" class="dropdown-item ai-icon">
                                        <svg id="icon-user1" xmlns="http://www.w3.org/2000/svg" width="18" height="18"
                                             viewbox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                                             stroke-linecap="round" stroke-linejoin="round" class="feather feather-user">
                                            <path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path>
                                            <circle cx="12" cy="7" r="4"></circle>
                                        </svg>
                                        <span class="ml-2"><b>@Session["Name"]</b></span>
                                    </a>
                                    <a href="/ParentPanel/EditProfile" class="dropdown-item ai-icon">
                                        <svg id="icon-inbox" xmlns="http://www.w3.org/2000/svg" width="18" height="18"
                                             viewbox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                                             stroke-linecap="round" stroke-linejoin="round" class="feather feather-mail">
                                            <path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z">
                                            </path>
                                            <polyline points="22,6 12,13 2,6"></polyline>
                                        </svg>
                                        <span class="ml-2">My Profile </span>
                                    </a>
                                    <a href="/Home/Login" class="dropdown-item ai-icon">
                                        <svg id="icon-logout" xmlns="http://www.w3.org/2000/svg" width="18" height="18"
                                             viewbox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2"
                                             stroke-linecap="round" stroke-linejoin="round"
                                             class="feather feather-log-out">
                                            <path d="M9 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h4"></path>
                                            <polyline points="16 17 21 12 16 7"></polyline>
                                            <line x1="21" y1="12" x2="9" y2="12"></line>
                                        </svg>
                                        <span class="ml-2">Logout </span>
                                    </a>
                                </div>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>

        <!--*****************  Sidebar start ************-->
        <div class="dlabnav">
            <div class="dlabnav-scroll">
                <ul class="metismenu" id="menu">
                    <li>
                        <a href="/ParentPanel/DashBoard" aria-expanded="false">
                            <i class="la la-home"></i>
                            <span class="nav-text">Dashboard</span>
                        </a>
                    </li>
                    <li>
                        <a class="has-arrow" href="javascript:void()" aria-expanded="false">
                            <i class="la la-users"></i>
                            <span class="nav-text">Leave</span>
                        </a>
                        <ul aria-expanded="false" class="mm-collapse">
                            <li>
                                <a class="has-arrow" href="javascript:void()" aria-expanded="false">Apply For Leave</a>
                                <ul aria-expanded="false" class="mm-collapse">
                                    <li><a href="/ParentPanel/ApplyLeave">Apply</a></li>
                                    <li><a href="/ParentPanel/LeaveList">List</a></li>
                                </ul>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a class="ai-icon" href="/ParentPanel/Complains" aria-expanded="false">
                            <i class="la la-calendar"></i>
                            <span class="nav-text">Complain Box</span>
                        </a>
                    </li>
                    <li>
                        <a class="ai-icon" href="/ParentPanel/AttendanceReport" aria-expanded="false">
                            <i class="la la-calendar"></i>
                            <span class="nav-text">Attendance Report</span>
                        </a>
                    </li>
                    <li>
                        <a class="ai-icon" href="/ParentPanel/FeeReport" aria-expanded="false">
                            <i class="la la-calendar"></i>
                            <span class="nav-text">Fee Report</span>
                        </a>
                    </li>
                    @*<li>
                        <a class="ai-icon" href="/ParentPanel/Homework" aria-expanded="false">
                            <i class="la la-calendar"></i>
                            <span class="nav-text">Homework</span>
                        </a>
                    </li>*@
                    <li>
                        <a class="ai-icon" href="/ParentPanel/SyllabusList" aria-expanded="false">
                            <i class="la la-calendar"></i>
                            <span class="nav-text">Syllabus</span>
                        </a>
                    </li>
                    <li>
                        <a class="ai-icon" href="/ParentPanel/TimeTableList" aria-expanded="false">
                            <i class="la la-calendar"></i>
                            <span class="nav-text">TimeTable</span>
                        </a>
                    <li>
                        <a class="ai-icon" href="/ParentPanel/ChangePassword" aria-expanded="false">
                            <i class="la la-calendar"></i>
                            <span class="nav-text">Change Password</span>
                        </a>
                    </li>
                    <li>
                        <a class="ai-icon" href="/ParentPanel/Homework" aria-expanded="false">
                            <i class="la la-calendar"></i>
                            <span class="nav-text">Homework</span>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        <!--*********** Sidebar end*************-->

        <div class="content-body">
            @RenderBody()
        </div>
        <!--*******************  Footer start ************-->
        <div class="footer">
            <div class="copyright">
                <p>Copyright © 2024 All right reserved</p>
            </div>
        </div>
        <!--************* Footer end ********-->
    </div>
    <!--****************Main wrapper end**********-->
    <!--*************** Scripts *******-->
    <!-- Required vendors -->
    <script src="~/AssetsAdmin/vendor/global/global.min.js"></script>
    <script src="~/AssetsAdmin/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script src="~/AssetsAdmin/js/custom.min.js"></script>

    <!-- Chart Morris plugin files -->
    <script src="~/AssetsAdmin/vendor/raphael/raphael.min.js"></script>
    <script src="~/AssetsAdmin/vendor/morris/morris.min.js"></script>


    <!-- Chart piety plugin files -->
    <script src="~/AssetsAdmin/vendor/peity/jquery.peity.min.js"></script>


    <!-- Demo scripts -->
    <script src="~/AssetsAdmin/js/dashboard/dashboard-2.js"></script>

    <!-- Datatable -->
    <script src="~/AssetsAdmin/vendor/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/AssetsAdmin/js/plugins-init/datatables.init.js"></script>

    <!-- Chart ChartJS plugin files -->
    <script src="~/AssetsAdmin/vendor/chart.js/Chart.bundle.min.js"></script>
    <script src="~/AssetsAdmin/vendor/chartist-plugin-tooltips/js/chartist-plugin-tooltip.min.js"></script>
    <script src="~/AssetsAdmin/js/plugins-init/chartjs-init.js"></script>

    <!-- Svganimation scripts -->
    <script src="~/AssetsAdmin/vendor/svganimation/vivus.min.js"></script>
    <script src="~/AssetsAdmin/vendor/svganimation/svg.animation.js"></script>




</body>

</html>