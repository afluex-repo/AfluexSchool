                <a class="nav-link dropdown-toggle" href="/TeacherLogin/Homework"><i class="material-icons icon">dashboard</i><span>Homework</span> </a>

            </li>
        </ul>
        <ul class="nav flex-column">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="/TeacherLogin/NoticeMaster"><i class="material-icons icon">dashboard</i><span>Notice</span> </a>

            </li>
        </ul>*@
       
        <ul class="nav flex-column">
