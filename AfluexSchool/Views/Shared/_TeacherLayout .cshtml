            {
                <ul class="nav flex-column">
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="/TeacherLogin/StudentAttendanceByTeacher"><i class="material-icons icon">dashboard</i><span>Student Attendance</span> </a>

                    </li>
                </ul>
            }
        }
        <ul class="nav flex-column">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="/TeacherLogin/AttendanceReport"><i class="material-icons icon">dashboard</i><span>Self Attendance Report</span> </a>

            </li>
        </ul>
        <ul class="nav flex-column">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="/TeacherLogin/EmployeeSalarySlip"><i class="material-icons icon">dashboard</i><span>Salary Slip</span> </a>

            </li>
        </ul>
